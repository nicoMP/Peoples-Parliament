{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@src/*": [
        "app/src/*"
      ],
      "@constants/*": [
        "app/constants/*"
      ],
      "@functions/*": [
        "./functions/*"
      ],
      "@hooks/*": [
        "./hooks/*"
      ],
      "@services/*": [
        "app/src/services/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}
